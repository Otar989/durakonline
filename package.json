{
  "name": "durakonline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "node server/index.ts",
  "lint": "eslint .",
  "lint:fix": "eslint . --fix",
  "typecheck": "tsc --noEmit",
    "dev:ws": "node server/index.ts",
    "dev:all": "concurrently -k -n WEB,WS -c magenta,cyan \"npm:dev\" \"npm:start\"",
  "test": "vitest run",
  "test:watch": "vitest",
  "format": "prettier . --write"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "4.7.5",
  "socket.io-client": "4.7.5",
  "zod": "3.23.8",
  "framer-motion": "11.2.10",
  "workbox-window": "7.1.0",
  "@sentry/nextjs": "8.18.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
  "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
  "tailwindcss": "^4",
  "typescript": "^5",
    "vitest": "^1.6.0",
    "husky": "^9.0.0",
  "lint-staged": "^15.2.0",
  "prettier": "3.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["eslint --fix"],
    "*.{ts,tsx}": ["tsc --noEmit --pretty false --skipLibCheck true"],
    "*.{md,css,json}": ["prettier --write"]
  },
  "engines": {
    "node": "20.x"
  }
}
